<launch>  
  <node pkg="spatial_visual_system" type="spatial_visual_system_node" name="svs" output="screen" launch-prefix="valgrind">
    <remap from="~image" to="/hsrb/head_rgbd_sensor/rgb/image_rect_color" />
    <remap from="~points" to="/hsrb/head_rgbd_sensor/depth/points" />
    <param name="yolo_results_topic" type="str" value="/gpu/ObjectServicesNodelet/lookup_objects" />
    <param name="set_frame_rate_service" type="str" value="/gpu/Republisher/set_frame_rates" />
    <param name="yolo_debug" type="bool" value="true" />
  </node>

</launch>
